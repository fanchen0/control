# Android/Gradle .gitignore

# IDE files
.idea/
*.iml
*.ipr
*.iws

# macOS
e.g.
.DS_Store

# Gradle
.gradle/
build/
app/build/

# Local configuration
local.properties

# NDK/External build
.externalNativeBuild/
.cxx/

# Logs and temporary files
*.log
*.tmp
*.swp
*.swo

# Android captures
captures/

